rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    
    
    match /books/{bid}/thumbnail.jpg {
      allow read :if true;
      allow write:if request.auth!=null;
    }
    
    match /books/{bid}/pages/{imageID} {
      allow read :if request.auth!=null;
      allow write:if request.auth!=null;
    }
    
  }
}
