service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /books {
      allow read:if true;
      allow update:if request.auth != null;
    }
     match /newspapers {
      allow read:if true;
      allow update:if request.auth != null;
    }
    match /photoalbums {
      allow read:if true;
      allow update:if request.auth != null;
    }
    match /notes {
      allow read:if true;
      allow update:if request.auth != null;
    }
    match /users {
    allow read:if request.auth != null;
    allow write:if request.auth != null;
    }
    
    
  }
}
