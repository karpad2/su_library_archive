{
  "name": "su-library-archive",
  "description": "Gradska digitalna biblioteka Subotica",
  "version": "0.1.0-alpha-prerelease",
  "author": "karpad2",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve .",
    "publishwin": "electron-builder  --win --linux -p always --publish never",
    "start": "electron ."
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "karpad2",
        "repo": "su_library_archive"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/android-chrome-512x512.png"
    },
    "productName": "Gradska digitalna biblioteka Subotica",
    "nsis": {
      "oneClick": false,
      "artifactName": "su_library_archive.setup.${version}.${ext}",
      "uninstallDisplayName": "Gradska digitalna biblioteka Subotica"
    },
    "linux": {
      "category": "karpad2.app.category.Education",
      "target": "AppImage",
      "icon": "src/assets/icons/android-chrome-512x512.png"
    },
    "appId": "com.github.karpad2.su_library_archive"
  },
  "dependencies": {
    "@vue/cli-plugin-pwa": "^4.5.15",
    "axios": "^0.22.0",
    "babel-plugin-remove-comments": "^2.0.0",
    "bootstrap-icons": "^1.5.0",
    "bootstrap-icons-vue": "^0.7.0",
    "bootstrap-vue": "^2.21.2",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.6.4",
    "crypto-js": "^4.1.1",
    "disable-devtool": "^0.1.5",
    "electron-log": "^4.4.3",
    "electron-notarize": "^1.1.1",
    "electron-updater": "^4.6.1",
    "express": "^4.17.1",
    "firebase": "^9.0.0-beta.7",
    "firebase-admin": "^10.0.0",
    "firebase-auth": "^0.1.2",
    "firebase-cli": "^1.2.0",
    "firebase-tools": "^9.16.5",
    "firebaseui": "^6.0.0",
    "fork-me-on-github-vue": "^1.0.7",
    "ionic": "^5.4.16",
    "latin-to-cyrillic": "^1.0.1",
    "lzma": "^2.3.2",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "pdf-image": "^2.0.0",
    "pdf2pic": "^2.1.4",
    "pdfjs": "^2.4.7",
    "register-service-worker": "^1.7.2",
    "sass-loader": "^12.4.0",
    "tf-idf": "^1.0.0",
    "uglify-js": "^3.14.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "v-calendar": "^2.3.2",
    "v-viewer": "^1.6.4",
    "vue": "^2.6.11",
    "vue_pdfjs_viewer": "^0.1.12",
    "vue-audio-recorder": "^3.0.1",
    "vue-awesome-countdown": "^1.1.4",
    "vue-breakpoints": "^1.1.0",
    "vue-cli-plugin-pwa": "^4.6.0",
    "vue-cookie-law": "^1.13.3",
    "vue-cookies": "^1.7.4",
    "vue-countdown": "^1.0.4",
    "vue-country-flag": "^2.2.0",
    "vue-inner-image-zoom": "^1.1.1",
    "vue-lazyload": "^1.3.3",
    "vue-material": "^1.0.0-beta-12",
    "vue-meta": "^2.4.0",
    "vue-password-strength-meter": "^1.7.2",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.5",
    "vue-social-sharing": "^3.0.8",
    "vue-webble": "^0.1.0",
    "vueditor": "^0.3.1",
    "vuejs-noty": "^0.1.3",
    "vuetify": "^2.5.10",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@webpack-cli/generators": "^2.3.0",
    "babel-eslint": "^10.1.0",
    "electron": "^16.0.4",
    "electron-builder": "^22.10.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.17.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^7",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.46.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "strict": "off",
      "no-unused-vars": "off",
      "no-mixed-spaces-and-tabs": "off",
      "vue/no-parsing-error": "off",
      "no-console": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 8"
  ]
}
